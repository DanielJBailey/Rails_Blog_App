<h1>Blog</h1>
<% @pages.each do |page| %>
    <h2><%=  page.title %></h2>
    <%= link_to "Show Page", page_path(page) %>
    <%= link_to "Edit", edit_page_path(page.id) %>
    <%= link_to "Delete", page_path(page.id), method: :delete %>
<% end %>
<p><%= link_to "Create New Page", new_page_path %></p>
